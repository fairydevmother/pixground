<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('partials/head'); %>
  <title>Get in Touch With Us Now</title>
  <link href="/assets/css/other.css" rel="stylesheet"/>
  <style>

  body{margin-top:20px;}

 </style>
</head>
<body>

<header>
  <%- include('partials/header'); %>
</header>


<button onclick="topFunction()" id="myBtn" title="Go to top"><img src="/assets/img/up-arrow.png" style="width:34px;" /></button>

<main class="vh-100">
  <section  style="padding-top:70px;">
    <div style="width:100%; text-align:center;">
        <div class="container">
          <div class="module">
            <div class="module-inside">
            <h1 class="innerT">Categories</h1>
             </div>
          </div>
         
        </div>
        <div class="container" style="margin-top:5rem;">
          <div class="row">
            <% items.forEach(function(image){ 
              const expression=image.category;
           if(typeof expression !== 'undefined' && expression !== 'null'){ 
           %>
           <div class="col-md-3 col-sm-4 col-xs-6">
            <div class="card card-favorite">
              <div class="card-img-container">
                <a href="/category/<%= expression %>" style="background-image:url('/assets/img/category.png')" class="card-img"></a>
              </div>  
              <div class="card-content">
                <div class="card-meta">
                  <span class="meta-pulse meta-box">
                    <h2>
                        <a href="/category/<%= expression %>">
                      <%= expression%> 
                  </a>   
                </h2>
                  </span>
                  
                </div>
                
              </div>
                
            </div>
          </div> 
           <% }
            }) 
            %>
          </div>    
        </div>
      </div>
</section>
    
   
</main>


<footer >
  <%- include('partials/footer'); %>
</footer>
</body>
</html>